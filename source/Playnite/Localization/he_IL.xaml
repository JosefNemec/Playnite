    <!--FirstTime Wizard-->
    <sys:String x:Key="LOCFirstWindowTitle">הגדרת הפעם הראשונה</sys:String>
    <sys:String x:Key="LOCFirstIntro" xml:space="preserve">האשף ינחה אתכם בתהליך ייבוא משחק אוטומטי והגדרה של ספריות משחק חיצוניות. Playnite יכול לייבא אוטומטית משחקים ממספר שירותים כמו Steam או GOG, וגם לשמור על הספריה שלכם מעודכנת על ידי עדכון אוטומטי בעליית האפליקציה.
 
זכרו שתמיד ניתן להוסיף משחק מותאם אישית לכל פלטפורמה ידנית מהתפריט הראשי על ידי לחיצה על כפתור ה 'Playnite'.
    </sys:String>
    <sys:String x:Key="LOCFirstDBLocDefault">מיקום ברירת מחדל עבור מידע התוכנה לכל המשתמשים</sys:String>
    <sys:String x:Key="LOCFirstDBLocCustom">מיקום מותאם אישית</sys:String>
    <sys:String x:Key="LOCFirstExternalTitle">אינטגרציית ספריה</sys:String>
    <sys:String x:Key="LOCFirstImportIntroduction" xml:space="preserve">ייבוא אוטומטי של משחקים מהשירותים הבאים. שינויי משחק עתידיים (מצב התקנה) יעודכנו אוטומטית בעליה של Playnite או שיופעלו ידנית. האפשרויות הנבחרות ישפיעו על הייבוא הראשוני וכל והבאים אחריו.</sys:String>
    <sys:String x:Key="LOCFirstSteamIntegration">אינטגרציית Steam</sys:String>
    <sys:String x:Key="LOCFirstOriginIntegration">אינטגרציית Origin</sys:String>
    <sys:String x:Key="LOCFirstGOGIntegration">אינטגרציית GOG</sys:String>
    <sys:String x:Key="LOCFirstBattleNetIntegration">אינטגרציית Battle.net</sys:String>
