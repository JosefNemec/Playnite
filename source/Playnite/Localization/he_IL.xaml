    <!--FirstTime Wizard-->
    <sys:String x:Key="LOCFirstWindowTitle">הגדרת הפעם הראשונה</sys:String>
    <sys:String x:Key="LOCFirstIntro" xml:space="preserve">האשף ינחה אתכם בתהליך ייבוא משחק אוטומטי והגדרה של ספריות משחק חיצוניות. Playnite יכול לייבא אוטומטית משחקים ממספר שירותים כמו Steam או GOG, וגם לשמור על הספריה שלכם מעודכנת על ידי עדכון אוטומטי בעליית האפליקציה.
        
זכרו שתמיד ניתן להוסיף משחק מותאם אישית לכל פלטפורמה ידנית מהתפריט הראשי על ידי לחיצה על כפתור ה 'Playnite'.
    </sys:String>
    <sys:String x:Key="LOCFirstDBLocation" xml:space="preserve">בחר מיקום לקובץ בסיס נתוני הספריה. ניתן לשנות את המיקום מאוחר יותר בתפריט ההגדרות.</sys:String>
    <sys:String x:Key="LOCFirstDBLocDefault">מיקום ברירת מחדל עבור מידע התוכנה לכל המשתמשים</sys:String>
    <sys:String x:Key="LOCFirstDBLocCustom">מיקום מותאם אישית</sys:String>
    <sys:String x:Key="LOCFirstExternalTitle">אינטגרציית ספריה</sys:String>
    <sys:String x:Key="LOCFirstImportIntroduction" xml:space="preserve">ייבוא אוטומטי של משחקים מהשירותים הבאים. שינויי משחק עתידיים (מצב התקנה) יעודכנו אוטומטית בעליה של Playnite או שיופעלו ידנית. האפשרויות הנבחרות ישפיעו על הייבוא הראשוני וכל והבאים אחריו.</sys:String>
    <sys:String x:Key="LOCFirstSteamIntegration">אינטגרציית Steam</sys:String>
    <sys:String x:Key="LOCFirstOriginIntegration">אינטגרציית Origin</sys:String>
    <sys:String x:Key="LOCFirstGOGIntegration">אינטגרציית GOG</sys:String>
    <sys:String x:Key="LOCFirstBattleNetIntegration">אינטגרציית Battle.net</sys:String>
    <sys:String x:Key="LOCFirstUplayIntegration">אינטגרציית Uplay</sys:String>
    <sys:String x:Key="LOCFirstSteamImportCategories">ייבא קטגוריות מחשבון:</sys:String>
    <sys:String x:Key="LOCFirstOtherGamesTitle">משחקים אחרים</sys:String>
    <sys:String x:Key="LOCFirstOtherGamesText">לחץ על הכפתור מתחת כדי לייבא משחקים מותקנים אחרים או לחפש אחריהם ידנית (כולל משחקי Microsoft Store).</sys:String>
    <sys:String x:Key="LOCFirstOtherGamesImportButton">ייבא משחקים</sys:String>
    <sys:String x:Key="LOCFirstConfigFinishedTitle">הסתיימה ההגדרה</sys:String>
    <sys:String x:Key="LOCFirstConfigFinishedText" xml:space="preserve">התקנה ראשונית הסתיימה. זכור שביכולתך לשנות את כל ההגדרות מאוחר יותר על ידי תפריט ה"הגדרות".

תוכלו גם להוסיף כל משחק אחר (כולל משחקי Microsoft Store) מאוחר יותר מהתפריט הראשי על ידי לחיצה על לוגו האפליקציה של Playnite.</sys:String>
    <sys:String x:Key="LOCFirstConfigOpenEmuImport">פתח מנחה ייבוא אמולציה</sys:String>
    <sys:String x:Key="LOCFirstConfigGridViewLayout">פריסת תצוגת רשת</sys:String>
    <sys:String x:Key="LOCFirstConfigGridViewLayoutDesc">בחרו פריסה מועדפת לתצוגת רשת ב Playnite. פריסת תמונות ניתנת לשינוי בכל זמן בהגדרות האפליקציה, כולל אפשרויות מתיחת תמונה.</sys:String>

